package colecoes;

import java.util.HashSet;

public class ConjuntoBaguncado {
	public static void main(String[] args) {
		HashSet conjunto = new HashSet();
		
		// converte double => Double (não aceita tipos primitivos)
		conjunto.add(1.2); 
		conjunto.add(true);
		conjunto.add("Teste");
		conjunto.add(1);
		conjunto.add('x');
		
		System.out.println("Tamanho é: " + conjunto.size());
		
		conjunto.add("Teste");
		
		System.out.println("Tamanho é: " + conjunto.size());
		
	}
}
